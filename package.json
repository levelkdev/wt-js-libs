{
  "name": "wt-js-libs",
  "version": "0.1.0",
  "description": "Javascript libraries to interact with the Winding Tree contracts",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "test-build": "./node_modules/mocha/bin/mocha test -- test-build",
    "postinstall": "scripts/install.sh",
    "testrpc": "./node_modules/ethereumjs-testrpc/bin/testrpc",
    "build": "webpack --config build.web.config.js && webpack --config build.node.config.js"
  },
  "keywords": [
    "winding-tree",
    "security",
    "encryption",
    "smart-contracts"
  ],
  "author": "Winding Tree Developers <dev@windingtree.com>",
  "license": "MIT",
  "browser": {
    "fs": false,
    "child_process": false
  },
  "dependencies": {
    "abi-decoder": "^1.0.8",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.5.0",
    "eth-lightwallet": "^2.5.4",
    "ethereumjs-util": "^5.1.2",
    "kbpgp": "^2.0.72",
    "truffle-hdwallet-provider": "0.0.3"
  },
  "devDependencies": {
    "ethereumjs-testrpc": "3.0.3",
    "truffle": "3.2.2",
    "webpack": "^2.4.1",
    "webpack-bundle-size-analyzer": "^2.7.0"
  }
}
